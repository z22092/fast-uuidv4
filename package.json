{
	"name": "fast-uuidv4",
	"version": "1.3.0",
	"description": "Hardware-accelerated uuidV4 gen",
	"main": "./build/Release/uuid.node",
	"exports": "./build/Release/uuid.node",
	"types": "./@types",
	"type": "module",
	"author": "Jefferson Venceslau",
	"license": "MIT",
	"allow-same-version": true,
	"git-tag-version": true,
	"scripts": {
		"install": "npx node-gyp-build",
		"test": "npm install && node test",
		"commit:all": "git add . && npm run commit",
		"commit:message": "npx --no-install cz",
		"release": "npx --no-install standard-version",
		"commit": "npm run commit:message"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/z22092/fast-uuidv4"
	},
	"keywords": [
		"uuid",
		"rfc4122"
	],
	"engines": {
		"node": ">= 12.0.0"
	},
	"dependencies": {
		"node-gyp-build": "^4.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"commitizen": "^4.2.4",
		"cz": "^1.8.2",
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^7.0.4",
		"standard-version": "^9.3.2"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
